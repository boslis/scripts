end)


-- Sell Holding Item One
FarmingSettings:Cheat("Button", "Sell Holding Item One", function()
local args = {
    [1] = "EndDialogue",
    [2] = {
        ["NPC"] = "Merchant",
        ["Option"] = "Option1",
        ["Dialogue"] = "Dialogue5"
    }
}

game:GetService("Players").LocalPlayer.Character.RemoteEvent:FireServer(unpack(args))
end)


-- Sell Holding Item All
FarmingSettings:Cheat("Button", "Sell Holding Item All", function()
    local args = {
        [1] = "EndDialogue",
        [2] = {
            ["NPC"] = "Merchant",
            ["Dialogue"] = "Dialogue5",
            ["Option"] = "Option2"
        }
    }
    
    game:GetService("Players").LocalPlayer.Character.RemoteEvent:FireServer(unpack(args))
end)


-- Item Farm
FarmingSettings:Cheat(
    "Checkbox",
    "Item Farm",
    function(State)
        print("Item Farm changed:", State)
        itemfarm = not itemfarm
        while itemfarm do
        loadstring(game:HttpGet("https://raw.githubusercontent.com/french-spy/roblox-scripts/main/yba%20item%20autofarm(getscriptclosure).lua"))();
        end
    end)


-- Special Reset
FarmingSettings2:Cheat("Button", "Special Reset", function()
    game.Players.LocalPlayer.Character.Humanoid.Health = 0
end)


-- Anti Vampire Burn
FarmingSettings2:Cheat("Button", "Anti Vampire Burn", function()
    while true do
        Workspace.Weather.Value = "null"
        wait(1)
      end
    end)

-- Click TP Tool
FarmingSettings2:Cheat("Button", "Click TP Tool", function()
    local A = true
local B = game.Players.LocalPlayer
local C = Instance.new('HopperBin')
C.Parent = B.Backpack
C.Name = 'Click TP'
local function D(E)
    if B == nil or B.Character == nil then
        return
    end
    B.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(E.x, E.y + 7, E.z))
end
A = true
local function F(G)
    if not A then
        return
    end
    if B == nil then
        return
    end
    A = false
    local H = G.Hit
    local I = H.lookVector
    D(H.p)
    wait()
    A = true
end
local function J(G)
    G.Icon = "rbxasset://textures\\ArrowCursor.png"
    G.Button1Down:Connect(function()
        F(G)
    end)
end
C.Selected:Connect(J)
end)
