-- brav 2k line srip
local Finity = loadstring(game:HttpGet("https://pastebin.com/raw/y4eeFHp0"))()

local FinityWindow = Finity.new(true)
FinityWindow.ChangeToggleKey(Enum.KeyCode.RightControl)

local Farming = FinityWindow:Category("boslis stuff")

local Skill = FinityWindow:Category("boslis skill tree")

local FarmingSettings = Farming:Sector("Farming")

local FarmingSettings2 = Farming:Sector("Misc")

local ST = Skill:Sector("Stands")

local FT = Skill:Sector ("Fighting Styles")

local CT = Skill:Sector ("Character")

local itemfarm = false

-- Anti AFK
local vu = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function()
   vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
   wait(1)
   vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)


-- TP Bypass
local TPBypass
TPBypass = hookfunction(getrawmetatable(game).__namecall, newcclosure(function(self, ...)
  local args = {...}
  if self.Name == "Returner" and args[1] == "idklolbrah2de"  then
          return "  ___XP DE KEY"
      end
  return TPBypass(self, ...)
end))

-- Use Arrow
FarmingSettings:Cheat("Button", "Use Arrow", function()
    local args = {
        [1] = "EndDialogue",
        [2] = {
            ["NPC"] = "Mysterious Arrow",
            ["Option"] = "Option1",
            ["Dialogue"] = "Dialogue2"
        }
    }
    
    game:GetService("Players").LocalPlayer.Character.RemoteEvent:FireServer(unpack(args))
end)


